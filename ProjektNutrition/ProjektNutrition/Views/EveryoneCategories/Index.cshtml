@model IEnumerable<ProjektNutrition.Models.Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/FrontEndView.cshtml";
}

<h2>Wybierz kategorie</h2>
<table class="table">
    <tr>
        <th>
            <h4> Nazwa</h4>
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <h4>@Html.DisplayFor(modelItem => item.name)</h4>
            </td>
            <td>
                <h4>@Html.ActionLink("Wybierz", "Index", "EveryoneProducts", new { id = item.id }, null)</h4>
            </td>

        </tr>

    }

</table>
