@model ProjektNutrition.Models.AspNetUserRoles

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Szczegóły</h2>

<div>
    <h4>Użytkownik-Rola</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nazwa Roli
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AspNetRoles.Name)
        </dd>

        <dt>
            E-mail użytkownika
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AspNetUsers.Email)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Modyfikuj", "Edit", new { id = Model.id }) |
    @Html.ActionLink("Powrót", "Index")
</p>
